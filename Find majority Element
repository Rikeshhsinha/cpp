#include <iostream>
#include <vector>
#include <climits>
using namespace std;

int majorityElemet(vector<int> num, int n)
{

    int freq = 0;
    int ans = 0;

    for (int i = 0; i < n; i++)
    {

        if (freq == 0)
        {
            ans = num[i];
        }
        if (ans == num[i])
        {
            freq++;
        }
        else
        {
            freq--;
        }
    }
    return ans;
}
int main()
{

    int n, target;

    cout << "Enter the number of item want to store :";
    cin >> n;

    vector<int> num;

    for (int i = 0; i < n; i++)
    {

        int x;

        cin >> x;

        num.push_back(x);
    }

    majorityElemet(num, n);

    int result = majorityElemet(num, n);
    cout << "Majority Element is: " << result << endl;

    return 0;
}
