#include <iostream>
#include <climits>
#include <vector>
using namespace std;

int main()
{
    int n;

    cout << "Enter the number of elements :";
    cin >> n;

    vector<int> marks;

    for (int i = 0; i < n; i++)
    {
        int x;

        cin >> x;

        marks.push_back(x);
    }

    int ans = 0;
    for (int val : marks)
    {

        ans = ans ^ val;
    }

    cout << "the single number is :" << ans << endl;

    return 0;
}
