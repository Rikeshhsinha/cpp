#include <iostream>
#include <climits>
using namespace std;

int main()
{
    int n, target;

    cout << "Enter the value of n :";
    cin >> n;

    cout << "Enter the value of target:";
    cin >> target;

    int marks[n];

    // if the target number not found
    int target_index = -1;

    for (int i = 0; i < n; i++)
    {

        cin >> marks[i];
    }
    for (int i = 0; i < n; i++)
    {

        if (marks[i] == target)
        {
            target_index = i;
        }
    }
    if (target_index != -1)
    {
        cout << "the target is :" << target << endl
             << "it's index is :" << target_index << endl;
    }
    else
    {
        cout << "Target is not found";
    }
    return 0;
}
