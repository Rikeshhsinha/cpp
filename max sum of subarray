#include <iostream>
#include <vector>
#include <climits>
using namespace std;

int main()
{

    int n;

    cout << "Enter the number of item want to store :";
    cin >> n;

    vector<int> num;

    for (int i = 0; i < n; i++)
    {

        int x;

        cin >> x;

        num.push_back(x);
    }

    int max_sum = INT_MIN;
    int current_sum = 0;
    for (int i = 0; i < n; i++)
    {
        current_sum += num[i];
        max_sum = max(current_sum, max_sum);

        if (current_sum < 0)
        {

            current_sum = 0;
        }
    }

    cout << "The max sum of subarray is:" << max_sum;

    return 0;
}
