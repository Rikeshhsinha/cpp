#include <iostream>
#include <climits>
using namespace std;

int main()
{
    int n;

    cout << "Enter the value of n :";
    cin >> n;

    int marks[n];

    for (int i = 0; i < n; i++)
    {

        cin >> marks[i];
    }

    int start = 0;
    int end = n - 1;

    while (start < end)
    {

        swap(marks[start], marks[end]);

        start++;
        end--;
    }
    for (int i = 0; i < n; i++)
    {

        cout << marks[i];
    }

    return 0;
}
